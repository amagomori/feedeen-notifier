{
    "name": "feedeen Notifier",
	"manifest_version": 2,
    "version": "1.1",
     "browser_action": {
	      "default_icon": "24.png",
	      "default_title": "open feedeen.com"
	 },
    "description": "Show unread count of feedeen.com", 
    "icons": {
        "24": "24.png", 
        "48": "48.png", 
        "96": "96.png"
    }, 
    "permissions": [
        "unlimitedStorage", 
        "notifications", 
        "tabs",
        "background",
        "alarms"
    ],
    "background": {
        "persistent": true,
	    "scripts": ["jquery-2.1.4.min.js", "background.js"]
	},
    "content_scripts": [
        {
            "matches": ["*://www.feedeen.com/*"],
            "js": ["jquery-2.1.4.min.js", "content.js"],
            "run_at": "document_end"
        }
    ]
}
